{% extends 'base.html.twig' %}

{% block title %}Modifier Profil{% endblock %}

{% block body %}
    <h2>Modifier mon profil</h2>

    {{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.nom) }}
    {{ form_row(form.prenom) }}
    {{ form_row(form.telephone) }}
    {{ form_row(form.mail) }}

    <button type="submit">Enregistrer</button>

    {{ form_end(form) }}
{% endblock %}
